{
    "name": "visual-analytics-app",
    "version": "1.0.0",
    "description": "Frontend for Electron and Web App",
    "main": "electron/main/index.js",
    "type": "module",
    "scripts": {
        "web": "vite --mode web",
        "ui": "cross-env VITE_APP_TARGET=electron vite --mode electron",
        "build:web": "vite build --mode web",
        "build:electron-renderer": "cross-env VITE_APP_TARGET=electron vite build --mode electron",
        "build:electron-main": "electron-builder --dir",
        "build:all": "npm run build:web && npm run build:electron-renderer && npm run build:electron-main",
        "package:electron": "npm run build:electron-renderer && electron-builder",
        "start": "electron . --dev",
        "startapp": "cross-env VITE_APP_TARGET=electron vite --mode electron & electron . --dev"
    },
    "build": {
        "linux": {
            "target": "AppImage",
            "category": "public.app-category.developer-tools"
        }
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.1",
        "@electron-toolkit/utils": "^4.0.0",
        "@primer/octicons-react": "^19.15.5",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@tailwindcss/vite": "^4.1.11",
        "@tanstack/react-query": "^5.83.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "electron-conf": "^1.3.0",
        "express": "^5.1.0",
        "lucide-react": "^0.533.0",
        "react-konva": "^19.0.7",
        "react-resizable-panels": "^3.0.3",
        "react-router-dom": "^7.7.1",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@types/node": "^24.1.0",
        "@vitejs/plugin-react": "^4.7.0",
        "autoprefixer": "^10.4.21",
        "cross-env": "^10.0.0",
        "electron": "^37.2.4",
        "electron-builder": "^26.0.12",
        "postcss": "^8.5.6",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "tailwindcss": "^4.1.11",
        "tw-animate-css": "^1.3.6",
        "vite": "^7.0.6"
    }
}